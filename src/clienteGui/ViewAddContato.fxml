            <Button fx:id="btnAddContato" layoutX="162.0" layoutY="153.0" mnemonicParsing="false" onAction="#executaAddContato" prefHeight="25.0" prefWidth="80.0" text="Add" />
            <Button fx:id="btnCancelarContato" layoutX="52.0" layoutY="153.0" mnemonicParsing="false" onAction="#cancelaAddContato" prefHeight="25.0" prefWidth="80.0" text="Cancelar" />
            <Text layoutX="98.0" layoutY="44.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Add contato">
               <font>
                  <Font size="18.0" />
               </font>
            </Text>
         </children>
      </Pane>
